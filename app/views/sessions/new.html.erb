<h1>Sign In</h1>
<%= form_tag sessions_path, {method: :post} do %>
  <div class="form-group">
    <%= label_tag :email %>
    <%= text_field_tag :email, "", class: "form-input" %>
  </div>
  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password, "", class:"form-input" %>
  </div>
  <div class="form-group">
    <%= submit_tag "Sign In" %>
  </div>
<% end %>
